{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/metadata/about",
      "dest": "/api/metadata-about.js",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/metadata/home",
      "dest": "/api/metadata-home.js",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/metadata/blog",
      "dest": "/api/metadata-blog.js",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],
  "functions": {
    "api/metadata-about.js": {
      "runtime": "nodejs14.x"
    },
    "api/metadata-home.js": {
      "runtime": "nodejs14.x"
    },
    "api/metadata-blog.js": {
      "runtime": "nodejs14.x"
    }
  }
}
